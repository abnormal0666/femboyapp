<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Femboy" Height="400" Width="400" ResizeMode="NoResize">
    <Window.Resources>
        <Style x:Key="ImageStyle1" TargetType="{x:Type Image}"/>
    </Window.Resources>
    <Image Style="{DynamicResource ImageStyle1}" HorizontalAlignment="Center" Height="380" VerticalAlignment="Center" Width="428" RenderTransformOrigin="-0.252,0.492" Source="/femboy.jpg" Stretch="Fill"/>
</Window>
